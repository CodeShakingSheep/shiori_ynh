{
    "name": "Shiori",
    "id": "shiori",
    "packaging_format": 1,
    "description": {
        "en": "Simple bookmark manager",
        "fr": "Gestionnaire de liens simple"
    },
    "version": "1.5.3~ynh4",
    "url": "https://github.com/go-shiori/shiori",
    "upstream": {
        "license": "MIT",
        "website": "https://github.com/go-shiori/shiori",
        "admindoc": "https://github.com/go-shiori/shiori/wiki",
        "userdoc": "https://github.com/go-shiori/shiori/wiki/Usage",
        "code": "https://github.com/go-shiori/shiori"
    },
    "license": "MIT",
    "maintainer": {
        "name": "eric_G",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 11.0.9"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/shiori",
                "default": "/shiori"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
